{
  "name": "iocane",
  "version": "1.0.2",
  "description": "Textual encryption library",
  "main": "dist/index.js",
  "types":"dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "docs": "jsdoc2md source/**/*.js > API.md",
    "format": "prettier --write \"{source,test}/**/*.js\"",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run build",
    "test": "run-s build test:coverage test:format",
    "test:coverage": "nyc --reporter=text mocha -r test/index.js 'test/{unit,integration}/**/*.spec.js' && nyc check-coverage --lines 50 --functions 50",
    "test:format": "prettier-check \"{source,test}/**/*.js\"",
    "test:integration": "mocha -r test/index.js 'test/integration/**/*.spec.js'",
    "test:unit": "mocha -r test/index.js 'test/unit/**/*.spec.js'",
    "test:unit:watch": "nodemon --exec 'npm run test:unit'"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "*.md",
    "*.jpg"
  ],
  "lint-staged": {
    "{source,test}/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perry-mitchell/iocane.git"
  },
  "keywords": [
    "crypto",
    "encrypt",
    "decrypt",
    "encryption",
    "buttercup",
    "text"
  ],
  "author": "Perry Mitchell <perry@perrymitchell.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/perry-mitchell/iocane/issues"
  },
  "homepage": "https://github.com/perry-mitchell/iocane#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "husky": "^3.1.0",
    "jsdoc-to-markdown": "^5.0.3",
    "lint-staged": "^9.5.0",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "prettier": "^1.19.1",
    "prettier-check": "^2.0.0",
    "sinon": "^7.5.0",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "pbkdf2": "~3.0.17"
  }
}
